<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.darksoft.kaife_cataapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kaife_cataAPP"
        tools:targetApi="31">
        <activity
            android:name=".ui.answers_after_quizz.AnswersAfterQuizzActivity"
            android:exported="false" />
        <activity
            android:name=".ui.answers.AnswersActivity"
            android:exported="false" />
        <activity
            android:name=".ui.quizz_global.QuizzGlobalActivity"
            android:exported="false" />
        <activity
            android:name=".ui.graphics.GraphicsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.statistics.StatisticsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.register_user_phone.RegisterUserPhoneActivity"
            android:exported="false" />
        <activity
            android:name=".ui.quizz.QuizzActivity"
            android:exported="false" />
        <activity
            android:name=".ui.sign_in_phone.SignInPhoneActivity"
            android:exported="false" />
        <activity
            android:name=".ui.splash_screen.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.sign_in.SignInActivity"
            android:exported="false" />
        <activity
            android:name=".ui.sign_up.SignUpActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>